<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.SignUpController">
   <children>
      <Button layoutX="36.0" layoutY="329.0" mnemonicParsing="false" onAction="#openLoginPage" style="-fx-background-color: #263F73;" text="Submit" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Button layoutX="212.0" layoutY="329.0" mnemonicParsing="false" onAction="#openWelcomePage" style="-fx-background-color: #263F73;" text="Welcome Page" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="26.0" layoutY="43.0" text="Username:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="26.0" layoutY="89.0" text="Password:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="26.0" layoutY="137.0" text="Confirm Password:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="30.0" layoutY="182.0" text="First Name:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="30.0" layoutY="226.0" text="Last Name:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTF" layoutX="212.0" layoutY="43.0" promptText="Enter username" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="firstNameTF" layoutX="212.0" layoutY="182.0" promptText="Enter first name" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="lastNameTF" layoutX="212.0" layoutY="226.0" promptText="Enter last name" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <PasswordField fx:id="passwordTF" layoutX="212.0" layoutY="89.0" promptText="Enter password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <PasswordField fx:id="cPasswordTF" layoutX="212.0" layoutY="137.0" promptText="Re-enter password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
   </children>
</AnchorPane>
