<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.LoginController">
   <children>
      <Label layoutX="89.0" layoutY="80.0" text="Username:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTF" layoutX="200.0" layoutY="79.0" promptText="Enter username" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: #263F73;" />
      <Label layoutX="89.0" layoutY="146.0" text="Password:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordTF" layoutX="200.0" layoutY="145.0" promptText="Enter password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <Button layoutX="155.0" layoutY="270.0" mnemonicParsing="false" onAction="#openDashboard" style="-fx-background-color: #263F73;" text="Submit" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Button layoutX="122.0" layoutY="337.0" mnemonicParsing="false" onAction="#openWelcomePage" style="-fx-background-color: #263F73;" text="Welcome Page" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
