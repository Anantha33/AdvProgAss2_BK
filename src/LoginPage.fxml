<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.LoginController">
   <children>
      <Label layoutX="87.0" layoutY="77.0" text="Username:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTF" layoutX="200.0" layoutY="77.0" promptText="Enter username" />
      <Label layoutX="89.0" layoutY="146.0" text="Password:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordTF" layoutX="200.0" layoutY="147.0" promptText="Enter password" />
      <Button layoutX="160.0" layoutY="272.0" mnemonicParsing="false" onAction="#openDashboard" style="-fx-background-color: black;" text="Submit" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="129.0" layoutY="337.0" mnemonicParsing="false" onAction="#openWelcomePage" style="-fx-background-color: black;" text="Welcome Page" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="errorMessage" alignment="CENTER" layoutX="55.0" layoutY="222.0" prefHeight="27.0" prefWidth="291.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
