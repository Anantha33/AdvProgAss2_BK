<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<?xml-model href=""?>
<BorderPane maxHeight="Infinity" maxWidth="Infinity" minHeight="300.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.LoginController">
  <top>
     <Button mnemonicParsing="false" onAction="#openWelcomePage" style="-fx-background-color: #263F73;" text="&lt;- Back" textFill="WHITE" BorderPane.alignment="CENTER_LEFT">
        <font>
           <Font name="Georgia" size="15.0" />
        </font>
        <BorderPane.margin>
           <Insets left="10.0" top="10.0" />
        </BorderPane.margin>
     </Button>
  </top>
  <center>
     <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
        <children>
           <Label text="Username:">
              <font>
                 <Font name="Georgia" size="20.0" />
              </font>
           </Label>
           <TextField fx:id="usernameTF" promptText="Enter username" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: #263F73;" />
           <Label text="Password:">
              <font>
                 <Font name="Georgia" size="20.0" />
              </font>
           </Label>
           <PasswordField fx:id="passwordTF" promptText="Enter password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
           <Button mnemonicParsing="false" onAction="#openDashboard" style="-fx-background-color: #263F73;" text="Submit" textFill="WHITE">
              <font>
                 <Font name="Georgia" size="20.0" />
              </font>
           </Button>
        </children>
        <padding>
           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
     </VBox>
  </center>
</BorderPane>