<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.WelcomeController">
   <children>
      <Label alignment="CENTER" layoutX="75.0" layoutY="36.0" prefHeight="58.0" prefWidth="251.0" text="Welcome to Burrito King!">
         <font>
            <Font name="Georgia" size="22.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="83.0" layoutY="118.0" text="Would you like to sign up?">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Button fx:id="signUpButton" alignment="CENTER" layoutX="152.0" layoutY="162.0" mnemonicParsing="false" onAction="#openSignUpPage" style="-fx-background-color: #263F73;" text="Sign Up" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="114.0" layoutY="258.0" text="Already a member?">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" alignment="CENTER" layoutX="162.0" layoutY="305.0" mnemonicParsing="false" onAction="#openLoginPage" style="-fx-background-color: #263F73;" text="Login" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
