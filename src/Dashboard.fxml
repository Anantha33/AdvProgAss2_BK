<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.DashboardController">
   <children>
      <Button layoutX="347.0" layoutY="398.0" mnemonicParsing="false" onAction="#openLoginPage" style="-fx-background-color: #263F73;" text="Logout" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="133.0" layoutY="15.0" prefHeight="32.0" prefWidth="108.0" text="Hey there,">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Label fx:id="userFullName" layoutX="241.0" layoutY="17.0" prefHeight="27.0" prefWidth="200.0">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="217.0" prefHeight="200.0" prefWidth="106.0">
         <children>
            <Button mnemonicParsing="false" onAction="#openCart" prefHeight="38.0" prefWidth="75.0" style="-fx-background-color: #263F73;" text="Cart" textFill="WHITE">
               <font>
                  <Font name="Georgia" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="profilePageButton" alignment="CENTER" mnemonicParsing="false" onAction="#openProfilePage" style="-fx-background-color: #263F73;" text="Profile" textFill="WHITE">
               <font>
                  <Font name="Georgia" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="viewOrderButton" mnemonicParsing="false" onAction="#viewAllOrders" prefHeight="33.0" prefWidth="213.0" style="-fx-background-color: #263F73;" text="All Orders" textFill="WHITE">
               <font>
                  <Font name="Georgia" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="upgradeUserButton" disable="true" mnemonicParsing="false" onAction="#openUpgradeProfile" style="-fx-background-color: #263F73;" text="Upgrade" textFill="WHITE">
               <font>
                  <Font name="Georgia" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
