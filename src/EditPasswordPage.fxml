<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="282.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BurritoKing_A2.EditProfileController">
   <children>
      <Label layoutX="117.0" layoutY="53.0" text="Edit Password">
         <font>
            <Font name="Georgia Bold Italic" size="22.0" />
         </font>
      </Label>
      <Label layoutX="18.0" layoutY="113.0" text="New Password:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <Button layoutX="157.0" layoutY="208.0" mnemonicParsing="false" onAction="#updatePassword" style="-fx-background-color: #263F73;" text="Submit" textFill="WHITE">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#openProfilePage" style="-fx-background-color: #263F73;" text="&lt;- Back" textFill="WHITE">
         <font>
            <Font name="Georgia" size="15.0" />
         </font>
      </Button>
      <Label layoutX="178.0" layoutY="290.0">
         <font>
            <Font name="Georgia" size="18.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="156.0" text="Confirm Password:">
         <font>
            <Font name="Georgia" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="newPasswordTF" layoutX="194.0" layoutY="113.0" prefHeight="25.0" prefWidth="180.0" promptText="Enter new password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
      <PasswordField fx:id="cNewPasswordTF" layoutX="194.0" layoutY="156.0" prefHeight="25.0" prefWidth="180.0" promptText="Re-enter new password" style="-fx-border-color: #263F73; -fx-border-width: 0px 0px 2px 0px;" />
   </children>
</AnchorPane>
